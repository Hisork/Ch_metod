      "source": [
        "Строим линейную модель:"
      ],
      "metadata": {
        "id": "UwCZ6fhfgsrU"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "N=len(cost_sort)\n",
        "mx = sum(delta_sort)/N\n",
        "my = cost_sort.sum()/N\n",
        "a2 = np.dot(delta_sort.T, delta_sort)/N\n",
        "a11 = np.dot(delta_sort.T, cost_sort)/N\n",
        "\n",
        "kn = (a11 - mx*my)/(a2 - mx**2)\n",
        "bn = my - kn*mx\n",
        "fn = np.array([kn*z+bn for z in delta_sort])"
      ],
      "metadata": {
